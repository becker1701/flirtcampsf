<%= provide(:title, "Resend Activation") %>

<h1>Resend Activation</h1>



<div class="row">
	<div class=" col-md-6 col-md-offset-3">

		<%= form_for :account_activation, url: account_activations_path, method: :post do |f| %>			

			<%= f.label :email %>
			<%= f.email_field :email, class: "form-control" %>

			<%= f.submit "Resend activation email", class: "btn btn-primary" %>
		<% end %>

	</div>
</div>
